    "content": "USE xingcel;\nSELECT * FROM Clientes LIMIT 10;\nSELECT * FROM Produtos;\nSELECT * FROM Vendas LIMIT 10;\nSELECT COUNT(*) AS total_clientes FROM Clientes;\nSELECT COUNT(*) AS total_clientes FROM Clientes;\nSELECT COUNT(*) AS total_produtos FROM Produtos;\nSELECT COUNT(*) AS total_vendas FROM Vendas;\nSELECT\n    v.id_venda,\n    v.data_venda,\n    c.nome AS nome_cliente,\n    c.cidade,\n    c.estado,\n    p.nome AS nome_produto,\n    p.categoria,\n    v.quantidade,\n    v.total_venda\nFROM\n    Vendas AS v\nJOIN\n    Clientes AS c ON v.id_cliente = c.id_cliente\nJOIN\n    Produtos AS p ON v.id_produto = p.id_produto\nORDER BY\n    RAND() -- Ordena aleatoriamente para obter uma amostra diferente a cada execução\nLIMIT 5;\nSELECT SUM(total_venda) AS faturamento_total FROM Vendas;\nSELECT\n    p.nome AS nome_produto,\n    SUM(v.quantidade) AS quantidade_total_vendida\nFROM\n    Vendas AS v\nJOIN\n    Produtos AS p ON v.id_produto = p.id_produto\nGROUP BY\n    p.nome\nORDER BY\n    quantidade_total_vendida DESC\nLIMIT 5;\nSELECT\n    c.estado,\n    SUM(v.total_venda) AS valor_total_vendas\nFROM\n    Vendas AS v\nJOIN\n    Clientes AS c ON v.id_cliente = c.id_cliente\nGROUP BY\n    c.estado\nORDER BY\n    valor_total_vendas DESC\nLIMIT 5;\n",
